            <string>draw_self();
draw_text(view_xview[0] + 32, view_yview[0] + 32, "Distance: " + string_format(distanceTravelled,0,0)+'#' +string_format(global.angle,0,0)+'#Top:' +string_format(topVert,0,0)+'#Bottom:' +string_format(bottomVert,0,0));

</string>
          </argument>
        </arguments>
      </action>
