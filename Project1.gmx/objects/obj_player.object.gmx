    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Step

key_jump = keyboard_check_pressed(vk_space);
key_rocket = keyboard_check(ord('F'));

key_left = keyboard_check(vk_left);
key_right = keyboard_check(vk_right);

global.angle +=  key_right - key_left;


if(vsp &lt;10) vsp +=grav;
if(hsp &gt;0) hsp -=0.05;
if(hsp&lt;0) hsp =0;

totalPower = 25;
calcAngle = degtorad(global.angle);

if(key_jump)
{
    start = true;
}

if(key_jump==1)
{   
    extraHoriz = totalPower * sin(calcAngle);
    extraVert = totalPower * cos(calcAngle);    
    vsp = -extraVert;
    hsp += extraHoriz;
}
if(key_rocket==1){
    vsp += -rocketspeed;
    hsp += rocketspeed;
}

if(hsp&gt;22)hsp=22;
if(vsp&gt;22)vsp=22;

//hoz collision
if(place_meeting(x+hsp,y, obj_wall)){
    while(!place_meeting(x+sign(hsp),y, obj_wall)){
        x+=sign(hsp);
    }
    hsp =0;
}

//vert collision
if(place_meeting(x,y + vsp, obj_wall)){
    while(!place_meeting(x,y+sign(vsp), obj_wall)){
        y+=sign(vsp);
    }
    vsp =0;
}

distanceTravelled += hsp;
if(start){
    x += hsp;
    y += vsp;
}
global.reset = keyboard_check_pressed(ord('R'));

if(global.reset == 1){
   
    x = originalX;
    y = originalY;
    distanceTravelled = 0;
    vsp = 0;
    hsp = 0;
    global.angle = 45;
    start =false;
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
