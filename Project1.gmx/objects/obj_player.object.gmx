        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Initialize 
vsp = 0;
hsp =0;
grav =0.8;
jumpspeed = 15;
distanceTravelled =0;
originalX =x;
originalY =y;
rocketspeed = 0.9;
start=false;
topVert = 0;
bottomVert = 1000;
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Step

key_jump = keyboard_check_pressed(vk_space);
key_rocket = keyboard_check(ord('F'));

if(vsp &lt;10) vsp +=grav;
if(hsp &gt;0) hsp -=0.05;
if(hsp&lt;0) hsp =0;

totalPower = 25;
if(start==false){
    image_angle = 360 - global.angle;
}
calcAngle = degtorad(global.angle);

if(key_jump)
{   
    start = true;    
    
    if(key_jump==1)
    {   
        extraHoriz = totalPower * sin(calcAngle);
        extraVert = totalPower * cos(calcAngle);    
        vsp = -extraVert;
        hsp += extraHoriz;
    }
    if(key_rocket==1){
        vsp += -rocketspeed;
        hsp += rocketspeed;
    }

}
//hoz collision
if(place_meeting(x+hsp,y, obj_wall)){
    while(!place_meeting(x+sign(hsp),y, obj_wall)){
        x+=sign(hsp);
    }
    hsp =0;
}

//vert collision
if(place_meeting(x,y + vsp, obj_wall)){
    //show_message("vsp = " + string(vsp));
    while(!place_meeting(x,y+sign(vsp), obj_wall)){
        y+=sign(vsp);
    }    
    vsp = -vsp+vsp/6+2;  
    if(abs(vsp)&lt;3){
        vsp=0;
    }
}

distanceTravelled += hsp;
if(start){
    if(vsp&gt;topVert)
    {
        topVert = vsp;
    }
    if(vsp&lt;bottomVert){
        bottomVert = vsp;
    }
    x += hsp;
    y += vsp;
    if(vsp!=0&amp;&amp;hsp!=0)
    {
        image_angle-=10;
    }
    else{
        image_angle =0;
    }
}
global.reset = keyboard_check_pressed(ord('R'));

if(global.reset == 1){
   
    x = originalX;
    y = originalY;
    distanceTravelled = 0;
    vsp = 0;
    hsp = 0;
    global.angle = 45;
    start =false;
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
